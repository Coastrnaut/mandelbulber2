&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  // square&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledBxFalse) zc.x = max(fabs(zc.x), fabs(zc.y));&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  // circle&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledOFalse) zc.x = sqrt((zc.x * zc.x) + (zc.y * zc.y));&lt;/span&gt;
