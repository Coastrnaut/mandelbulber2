&lt;span style=&quot;color:#2929cc&quot;&gt;    if (!fractal-&amp;gt;transformCommon.functionEnabledYFalse)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;      e = clamp(sqrt(cir.x * cir.x + cir.y * cir.y * fractal-&amp;gt;transformCommon.scaleA1) - e, 0.0, 100.0); // circle,&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    else&lt;/span&gt;
