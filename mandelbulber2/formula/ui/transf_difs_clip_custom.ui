&lt;span style=&quot;color:#2929cc&quot;&gt;  // base plane&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  double a = 1000.0;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledBFalse)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  {&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    a = (c.z - fractal-&amp;gt;transformCommon.offsetA0);&lt;/span&gt;
