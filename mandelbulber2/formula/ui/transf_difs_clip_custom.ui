&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledCy)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  {&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    CVector4 rec = zc;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    if (fractal-&amp;gt;transformCommon.functionEnabledEFalse)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;      rec.x = fabs(rec.x) - ((rec.y) * fractal-&amp;gt;transformCommon.scaleE1);&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    if (fractal-&amp;gt;transformCommon.functionEnabledXFalse)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;      rec.x = rec.x - (fabs(rec.y) * fractal-&amp;gt;transformCommon.scaleF1);&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    CVector4 f = fabs(rec);&lt;/span&gt;
