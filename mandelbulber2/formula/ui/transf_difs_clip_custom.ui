&lt;span style=&quot;color:#2929cc&quot;&gt;  }&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  aux.DE0 = (max(plD, aux.DE0) - fractal-&amp;gt;transformCommon.offset0005)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;      / (aux.DE * fractal-&amp;gt;analyticDE.scale1);&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
