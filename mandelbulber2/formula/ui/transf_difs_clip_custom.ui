&lt;span style=&quot;color:#2929cc&quot;&gt;  zc.z *= fractal-&amp;gt;transformCommon.scale3D111.z;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledFFalse)&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;    zc.x = zc.x + sin(zc.y) * fractal-&amp;gt;transformCommon.scale3D000.x;&lt;/span&gt;
&lt;span style=&quot;color:#2929cc&quot;&gt;  if (fractal-&amp;gt;transformCommon.functionEnabledGFalse)&lt;/span&gt;
